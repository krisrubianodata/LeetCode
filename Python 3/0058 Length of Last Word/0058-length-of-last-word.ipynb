    "class Solution:\n",
    "    def lengthOfLastWord(self, s: str) -> int:\n",
    "        words = s.split()\n",
    "        \n",
    "        if not words:\n",
    "            return 0\n",
    "        \n",
    "        return len(words[-1])"
   ]
