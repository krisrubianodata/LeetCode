    "class Solution:\n",
    "    def addBinary(self, a: str, b: str) -> str:\n",
    "        a, b = int(a, 2), int(b, 2)\n",
    "\n",
    "        while b:\n",
    "            n = a ^ b\n",
    "            y = (a & b) << 1\n",
    "            a, b = n, y\n",
    "\n",
    "        return bin(a)[2:]"
   ]
