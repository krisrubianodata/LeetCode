    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        memo = {1:1, 2:2}\n",
    "        def f(n):\n",
    "        if n in memo:\n",
    "            return memo[n]\n",
    "        else:\n",
    "            memo[n] = f(n-2) + f(n-1)\n",
    "            return memo[n]\n",
    "        \n",
    "        return f(n)"
   ]
